{
  "name": "colinhdev/cplot",
  "require": {
    "pocketmine/pocketmine-mp": "dev-stable as 4.0.0",
    "nethergamesmc/bedrock-data": "dev-master",
    "nethergamesmc/bedrock-protocol": "dev-master",
    "sof3/await-generator": "^3.4.1",
    "sof3/libasynql": "^4.0.1",
    "sofe/capital": "^0.1.2",
    "sof3/await-std": "@dev",
    "cooldogedev/bedrockeconomy": "^2.0.8",
    "colinhdev/libasyncevent": "^1.0.0",
    "entengamesde/cloudbridge": "dev-main",
    "entengamesde/managersystem": "dev-main"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.6.7",
    "phpstan/phpstan-strict-rules": "^1.2.3",
    "phpstan/extension-installer": "^1.1"
  },
  "repositories": {
    "pocketmine-mp": {
      "type": "vcs",
      "url":  "git@github.com:EntenGamesDE/PocketMine-MP.git"
    },
    "bedrock-protocol": {
      "type": "vcs",
      "url":  "git@github.com:NetherGamesMC/BedrockProtocol.git"
    },
    "bedrock-data": {
      "type": "vcs",
      "url":  "git@github.com:NetherGamesMC/BedrockData.git"
    },
    "capital": {
      "type": "vcs",
      "url": "https://github.com/SOF3/Capital"
    },
    "bedrockeconomy": {
      "type": "package",
      "package": {
        "name": "cooldogedev/bedrockeconomy",
        "version": "2.0.8",
        "source": {
          "type": "git",
          "url": "https://github.com/cooldogedev/BedrockEconomy",
          "reference": "main"
        }
      }
    },
    "cloudbridge": {
      "type": "vcs",
      "url":  "git@github.com:EntenGamesDE/CloudBridge.git"
    },
    "managersystem": {
      "type": "vcs",
      "url":  "git@github.com:EntenGamesDE/ManagerSystem.git"
    }
  }
}